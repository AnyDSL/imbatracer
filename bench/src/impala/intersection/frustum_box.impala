fn intersect_frustum_box(f: Frustum, b: Box, intr: fn () -> ()) -> () @{
    // Fast frustum-box intersection (only extreme-point test)
    for i in range(0, 4) @{
        let n = vec3_select(b.min, b.max, |j| f.planes(i).values(j) < 0.0f);
        if vec4_dot(f.planes(i), vec4_from_vec3(n, 1.0f)) > 0.0f @{
            return()
        }
    }

    intr()
}

