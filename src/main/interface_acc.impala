
extern fn impala_render(buf : &mut[u32], width : int, height : int, measureTime : bool, state : &mut State) -> ()
{
    for x, y, rtstate, scratch in renderImage(buf, width, height, measureTime, state) {
        @runIntegrator(&rtstate.integrator, &rtstate.scene, runCamera(&mut (rtstate.cam), x, y), scratch)
    }
}

